@model New.Models.Diary

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-6">
        <div class="form-group row">
            <label class="control-label col-sm-3"></label>
            <div class="col-sm-9">
                <h5>//Hapus data aktifitas?</h5>
            </div>
        </div>

        <div class="form-group row">
            <label class="control-label col-sm-3">Periode</label>
            <label class="control-label col-sm-9 border-bottom">@Model.IdPeriode</label>
        </div>
        <div class="form-group row">
            <label class="control-label col-sm-3">Nip</label>
            <label class="control-label col-sm-9 border-bottom">@Model.Nip</label>
        </div>
        <div class="form-group row">
            <label class="control-label col-sm-3">Tanggal</label>
            <label class="control-label col-sm-3 border-bottom">@Model.Tanggal.ToShortDateString()</label>
            <label class="control-label col-sm-3 border-bottom">@Model.WaktuMulai</label>
            <label class="control-label col-sm-3 border-bottom">@Model.WaktuHingga.ToString()</label>
        </div>
        <div class="form-group row">
            <label class="control-label col-sm-3">Aktifitas</label>
            <label class="control-label col-sm-9 border-bottom">@Model.Aktifitas</label>
        </div>
        <div class="form-group row">
            <label class="control-label col-sm-3">Tempat</label>
            <label class="control-label col-sm-9 border-bottom">@Model.Tempat</label>
        </div>
        <div class="form-group row">
            <label class="control-label col-sm-3">Hasil</label>
            <label class="control-label col-sm-9 border-bottom">@Model.Hasil    </label>
        </div>
        <form asp-action="Delete">
            <label class="control-label col-sm-3"></label>
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Hapus" class="btn btn-success" />
            <a asp-action="Index" class="btn btn-danger">Kembali</a>
        </form>
    </div>
</div>


