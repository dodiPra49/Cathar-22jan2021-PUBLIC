@model New.Models.PeriodeKaSKPD

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Apakah anda yakin menghapus data ini?</h4>
<hr />

