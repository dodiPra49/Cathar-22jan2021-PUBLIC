
@model IEnumerable<New.Models.Jabatan>

<h1></h1>
@{
    ViewData["SubDiv"] = ViewBag.SubDiv;
    ViewData["IdUnitKerja"] = ViewBag.IdUnitKerja;
}

@if (Model.Count() != 0)
{

    <table class="table table-striped table-sm">
        <thead class="thead-light">
            <tr>
                <th>
                    <div style="color:#007bff">ID</div>
                </th>
                <th>
                    <div style="color:#007bff">URAIAN</div>
                </th>
                <th>
                    <div style="color:#007bff">ESELON</div>
                </th>
                <th>
                    <div style="color:#007bff">ID ATAS</div>
                </th>
                <th>
                    <div style="color:#007bff">AKSI</div>
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Uraian)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IdEselon)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IdAtas)
                    </td>
                    <td>
                        <a asp-action="Index" asp-route-IdUnitKerja="@item.IdUnitKerja" asp-route-SubDiv="3" asp-route-IdJabatan="@item.Id" class="btn btn-success btn-sm"><i class="fa fa-edit"></i></a>
                        <a asp-action="Index" asp-route-IdUnitKerja="@item.IdUnitKerja" asp-route-SubDiv="4" asp-route-IdJabatan="@item.Id" class="btn btn-danger btn-sm"><i class="fa fa-times"></i></a>
                        @*<a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                            <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>*@
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <h5>Tidak ada record</h5>
}
