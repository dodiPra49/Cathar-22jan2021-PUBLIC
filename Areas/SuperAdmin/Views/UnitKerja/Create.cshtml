@model New.Models.UnitKerja

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Tambah Unit Kerja/ SKPD</h4>
<hr />

<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input asp-for="IdLevel" value="0" type="hidden" />

    @*<div class="form-group row">
        <label class="control-label col-sm-2"></label>
        <div class="col-sm-4">
            <h4>//Unit Kerja .(add)</h4>
        </div>
    </div>*@

    <div class="form-group row">
        <label asp-for="Id" class="control-label col-sm-2"></label>
        <div class="col-sm-4">
            <input asp-for="Id" class="form-control" />
            <span asp-validation-for="Id" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Uraian" class="control-label col-sm-2"></label>
        <div class="col-sm-4">
            <input asp-for="Uraian" class="form-control" />
            <span asp-validation-for="Uraian" class="text-danger"></span>
        </div>
    </div>

    <div class="form-group row">
        <label class="control-label col-sm-2">Valid Till</label>
        <div class="col-sm-4">
            <input asp-for="ValidTill" class="form-control" />
            <span asp-validation-for="ValidTill" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label class="control-label col-sm-2"></label>
        <div class="col-sm-2">
            <input type="submit" value="Simpan" class="btn btn-primary" />
            <a asp-action="Index" class="btn btn-success">Kembali</a>
        </div>
    </div>
</form>



