@model New.Models.Periode

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />

<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    
        <h4>Tambah Periode</h4>
        <hr />
    

    <div class="form-group row">
        <label class="control-label col-sm-2">Id</label>

        <input asp-for="Id" class="form-control col-sm-4" />
        <span asp-validation-for="Id" class="text-danger"></span>

    </div>
    <div class="form-group row">
        <label class="control-label col-sm-2">Tahun Periode</label>

        <input asp-for="Tahun" class="form-control col-sm-4" />
        <span asp-validation-for="Tahun" class="text-danger"></span>

    </div>
    <div class="form-group row">
        <label class="control-label col-sm-2">Bulan Periode  </label>

        <input asp-for="Bulan" class="form-control col-sm-4" />
        <span asp-validation-for="Bulan" class="text-danger"></span>

    </div>
    <div class="form-group row">
        <label class="control-label col-sm-2"></label>
        
            <input type="submit" value="Simpan" class="btn btn-primary" />&nbsp;
            <a class="btn btn-success" asp-action="Index">Kembali</a>
        
    </div>
</form>



