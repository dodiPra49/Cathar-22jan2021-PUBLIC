@model New.Models.HariKerjaStatus

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Apakah anda yakin menghapus data ini?</h4>

<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-1">
            Id
        </dt>
        <dd class="col-sm-4 border-bottom">
            @Html.DisplayFor(model => model.Id)
        </dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-1">
            Uraian
        </dt>
        <dd class="col-sm-4 border-bottom">
            @Html.DisplayFor(model => model.Uraian)
        </dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-1">
            Libur
        </dt>
        <dd class="col-sm-4 border-bottom">
            @Html.DisplayFor(model => model.Libur)
        </dd>
    </dl>
    <form asp-action="Delete">
        <div class="form-group row">
            <input type="hidden" asp-for="Id" />
            <div class="col-sm-1"></div>
            <div class="col-sm-2">
                <input type="submit" value="Delete" class="btn btn-danger" />
                <a class="btn btn-success" asp-action="Index">Kembali</a>
            </div>
        </div>
    </form>
</div>
