@model New.Models.JamKerjaPola

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Apakah anda yakin menghapus pola jam kerja ini?</h4>
<hr />

<div class="row">
    <div class="col-md-6">

        @*<div class="form-group row">
                <label class="control-label col-sm-3"></label>
                <div class="col-sm-9">
                    <h5>//Jam Kerja Pola (delete)</h5>
                </div>
            </div>*@

        @*Data Perorangan*@
        <div class="row">
            <label class="control-label col-sm-3">Id</label>
            <label class="control-label col-sm-9 border-bottom">@Model.Id</label>
        </div>

        <div class="row">
            <label class="control-label col-sm-3">Uraian</label>
            <label class="control-label col-sm-9 border-bottom">@Model.Uraian</label>
        </div>
        <div class="row">
            <label class="control-label col-sm-3">Valid Till</label>
            <label class="control-label col-sm-9 border-bottom">@Model.ValidTill.ToString()</label>
        </div>

        <form asp-action="Delete">
            <div class="form-group row">
                <label class="control-label col-sm-3"></label>
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="Hapus" class="btn btn-danger" />
                &nbsp;
                <a class="btn btn-success" asp-action="Index">Kembali</a>
            </div>
        </form>

    </div>
</div>  